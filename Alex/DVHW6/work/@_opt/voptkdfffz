library verilog;
use verilog.vl_types.all;
entity ld_st_rg_struct_generate is
    generic(
        n               : integer := 4
    );
    port(
        set             : in     vl_logic;
        clk             : in     vl_logic;
        clr             : in     vl_logic;
        sl_in           : in     vl_logic_vector;
        l_s             : in     vl_logic;
        sl_out          : out    vl_logic_vector
    );
    attribute mti_svvh_generic_type : integer;
    attribute mti_svvh_generic_type of n : constant is 1;
end ld_st_rg_struct_generate;
